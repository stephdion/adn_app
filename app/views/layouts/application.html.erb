<!DOCTYPE html>
<html lang="en">
    <head>
      <title><%= full_title(yield(:title)) %></title>
      <%= stylesheet_link_tag    "application", media: "all" %>
      <%= stylesheet_link_tag    "application-print", media: 'print' %>
      <%= javascript_include_tag "application" %>
      <style>
      a:link:after, a:visited:after {
  //content: " (" attr(href) ")";
  content: none !important;
font-size: 90%;
}
</style>
 	  <script src="//use.typekit.net/ueh4vwr.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
    <script src="//use.typekit.net/hhy3fxx.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
      <%= csrf_meta_tags %>
      <%= render 'layouts/shim' %>
    </head>
    <body>
    <span id="print-brand"><%= image_tag("idathletique/logo_print.jpg")  %></span>
        <% if Rails.env.production? %>
          <script>
              (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
              })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
              ga('create', 'UA-48040812-1', 'solutionadn.com');
              ga('send', 'pageview');
          </script>
        <% end %>

        <%= render 'layouts/header' %>

        <div class="container profile_master_style"
        <%if current_page?('/') || current_page?(controller: 'users', action: 'index')%>
         style="padding-left:0px;padding-right:0px;"
        <%end%>
        >
              <% flash.each do |key, value| %>
                  <%= content_tag(:div, value, class: "alert alert-#{key}") %>
                <% end %>
            <%= yield %>
        </div>

        <%= render 'layouts/footer' %>
        <%#= debug(@current_organization) %>
        <%#= debug(params) if Rails.env.development? %>
        <%#= debug(self.current_user) if Rails.env.development? %>
        <%= console if Rails.env.development? %>
    </body>
</html>
