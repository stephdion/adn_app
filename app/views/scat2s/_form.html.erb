<script type="text/javascript">

    var FormControl = {
        init: function () {
            //Symptoms
            <% $scat2_symptoms.each do |value| %>
                <% [0,1,2,3,4,5,6].each do |idx| %>
                    $("#symp_<%= value[1] %>_<%= idx %>").get(0).addEventListener('change', FormControl.changeSymptoms, false);
                <% end %>
            <% end %>
            //Sign Physical
            $("#scat2_unconsciousness_1").get(0).addEventListener('change', FormControl.changePhysical, false);
            $("#scat2_unconsciousness_0").get(0).addEventListener('change', FormControl.changePhysical, false);
            $("#scat2_balance_problem_1").get(0).addEventListener('change', FormControl.changePhysical, false);
            $("#scat2_balance_problem_0").get(0).addEventListener('change', FormControl.changePhysical, false);

            //Glassgow
            <% ['eye', 'verbal', 'mouvement'].each do |code| %>
            $("#scat2_glasgow_<%= code %>").get(0).addEventListener('change', FormControl.changeGlasgow, false);
            <% end %>

            //Sign Maddocks
            <% ['state', 'half_time', 'last_goal', 'last_team', 'last_win'].each do |code| %>
            $("#scat2_maddocks_<%= code %>").get(0).addEventListener('change', FormControl.changeMaddocks, false);
            <% end %>

            //Sign Cognitive
            <% ['month', 'date', 'day', 'year', 'hrs'].each do |code| %>
            $("#scat2_cognitive_<%= code %>").get(0).addEventListener('change', FormControl.changeCognitive, false);
            <% end %>

            //Sign Memory
            <% (1..15).each do |i| %>
            $("#memory_<%= i %>").get(0).addEventListener('change', FormControl.changeMemory, false);
            <% end %>

            //Sign concentration
            <% (1..5).each do |i| %>
            $("#concentration_<%= i %>").get(0).addEventListener('change', FormControl.changeConcentration, false);
            <% end %>

            //Sign stability
            <% ['two_feet', 'one_foot', 'feet_aligned'].each do |code| %>
            $("#scat2_stability_<%= code %>").get(0).addEventListener('change', FormControl.changeStability, false);
            <% end %>

            //Sign coordination
            $("#scat2_coordination").get(0).addEventListener('change', FormControl.changeCoordination, false);

            //Sign cognition
            <% ['elbow', 'candle', 'baby', 'finger', 'apple', 'paper', 'monkey', 'money',
                'carpet', 'sugar', 'perfume', 'cover', 'saddle', 'sandwich', 'sunset',
                'lime', 'bubble', 'wagon', 'iron', 'insect' ].each do |code| %>
            $("#cognition_<%= code %>").get(0).addEventListener('change', FormControl.changeCognition, false);
            <% end %>

            //raport
            $("#rep_score_symptoms").get(0).addEventListener('change', FormControl.changeReport, false);

            // first load
            FormControl.changeSymptoms();
            FormControl.changePhysical();
            FormControl.changeGlasgow();
            FormControl.changeMaddocks();
            FormControl.changeMemory();
            FormControl.changeConcentration();
            FormControl.changeStability();
            FormControl.changeCoordination();
            FormControl.changeCognition();
            FormControl.changeCognitive();


        },


        changeSymptoms: function (event) {
            var nbSymptoms = 0;
            var scoreSeveritySymptoms = 0;
            <% $scat2_symptoms.each do |value| -%>
                <% [0,1,2,3,4,5,6].each do |idx| %>
                    value = $("#symp_<%= value[1] %>_<%= idx %>").get(0);
                    if (value.checked) {
                        scoreSeveritySymptoms += parseInt(value.value);
                        nbSymptoms++;
                    }
                <% end %>
            <% end %>
            $("#symptoms_nb").html(nbSymptoms);
            $("#score_symptoms").html(22 - nbSymptoms)
            $("#symptoms_score_severity").html(scoreSeveritySymptoms);
            FormControl.changeReport();
        },

        changePhysical: function (event) {
            var scorePhysical = 0;
            //debugger;
            if ($("#scat2_unconsciousness_1").get(0).checked) {
                scorePhysical++
            }
            if ($("#scat2_balance_problem_1").get(0).checked) {
                scorePhysical++
            }


            $("#score_physical").html(scorePhysical);
            FormControl.changeReport();
        },

        changeGlasgow: function (event) {
            var scoreGlasgow = 0;
            <% ['eye', 'verbal', 'mouvement'].each do |code| %>
            value = $("#scat2_glasgow_<%= code %>").get(0).value;
            if (value != undefined && value != '') {
                scoreGlasgow += parseInt(value);
            }
            <% end %>
            $("#score_glasgow").html(scoreGlasgow);
            FormControl.changeReport();
        },

        changeMaddocks: function (event) {
            var scoreMaddocks = 0;
            <% ['state', 'half_time', 'last_goal', 'last_team', 'last_win'].each do |code| %>
            if ($("#scat2_maddocks_<%= code %>").is(':checked')) {
                scoreMaddocks++;
            }
            <% end %>
            //console.log("scoreMaddocks" + scoreMaddocks);
            $("#score_maddocks").html(scoreMaddocks);
            FormControl.changeReport();
        },

        changeCognitive: function (event) {
            var scoreCognitive = 0;
            <% ['month', 'date', 'day', 'year', 'hrs'].each do |code| %>
            if ($("#scat2_cognitive_<%= code %>").is(':checked')) {
                scoreCognitive++;
            }
            <% end %>

            $("#score_cognitive").html(scoreCognitive);
            FormControl.changeReport();
        },

        changeMemory: function (event) {
            var scoreMemory = 0;
            <% (1..15).each do |i| %>
            if ($("#memory_" +<%= i %>).is(':checked')) {
                scoreMemory++;
            }
            <% end %>

            $("#score_memory").html(scoreMemory);
            FormControl.changeReport();
        },

        changeConcentration: function (event) {
            var scoreConcentration = 0;
            <% (1..5).each do |i| %>
            if ($("#concentration_" +<%= i %>).is(':checked')) {
                scoreConcentration++;
            }
            <% end %>

            $("#score_concentration").html(scoreConcentration);
            FormControl.changeReport();
        },

        changeStability: function (event) {
            var scoreStability = 0;
            <% ['two_feet', 'one_foot', 'feet_aligned'].each do |code| %>
            value = $("#scat2_stability_<%= code %>").get(0).value;
            if (value != undefined && value != '') {
                scoreStability += parseInt(value);
            }
            <% end %>

            $("#score_stability").html(scoreStability);
            FormControl.changeReport();
        },

        changeCoordination: function (event) {
            var scoreCoordination = 0;
            if ($("#scat2_coordination").is(':checked')) {
                scoreCoordination++;
            }

            $("#score_coordination").html(scoreCoordination);
            FormControl.changeReport();
        },

        changeCognition: function (event) {
            var scoreCognition = 0;
            <% ['elbow', 'candle', 'baby', 'finger', 'apple', 'paper', 'monkey', 'money',
                'carpet', 'sugar', 'perfume', 'cover', 'saddle', 'sandwich', 'sunset',
                'lime', 'bubble', 'wagon', 'iron', 'insect' ].each do |code| %>
            if ($("#cognition_<%= code %>").is(':checked')) {
                scoreCognition++;
            }
            <% end %>

            if (scoreCognition > 5) {
                scoreCognition = 5;
            }
            $("#score_cognition").html(scoreCognition);
            FormControl.changeReport();
        },

        changeReport: function (event) {
            //debugger;
            $("#rep_score_symptoms").html($("#score_symptoms").get(0).innerText);
            $("#rep_score_physical").html($("#score_physical").get(0).innerText);
            $("#rep_score_glasgow").html($("#score_glasgow").get(0).innerText);
            $("#rep_score_stability").html($("#score_stability").get(0).innerText);
            $("#rep_score_coordination").html($("#score_coordination").get(0).innerText);
            rep_total = 0;
            rep_total += parseInt($("#score_symptoms").get(0).innerText);
            rep_total += parseInt($("#score_physical").get(0).innerText);
            rep_total += parseInt($("#score_glasgow").get(0).innerText);
            rep_total += parseInt($("#score_stability").get(0).innerText);
            rep_total += parseInt($("#score_coordination").get(0).innerText);
            $("#rep_total").html(rep_total);

            $("#rep_score_cognitive").html($("#score_cognitive").get(0).innerText);
            $("#rep_score_memory").html($("#score_memory").get(0).innerText);
            $("#rep_score_concentration").html($("#score_concentration").get(0).innerText);
            $("#rep_score_cognition").html($("#score_cognition").get(0).innerText);
            rep_total_sac = 0;
            rep_total_sac += parseInt($("#score_cognitive").get(0).innerText);
            rep_total_sac += parseInt($("#score_memory").get(0).innerText);
            rep_total_sac += parseInt($("#score_concentration").get(0).innerText);
            rep_total_sac += parseInt($("#score_cognition").get(0).innerText);
            $("#rep_total_sac").html(rep_total_sac);

            $("#rep_total_sac2").html(rep_total_sac + rep_total);

            $("#rep_score_maddocks").html($("#score_maddocks").get(0).innerText);

        }
    };

    FormControl.init();
</script>
