<% provide(:title, t(:subcategory_edit)) %>
<div class="margin-bottom">
	<h1 class="text-center"><%= I18n.t(:subcategory_edit) %></h1>
</div>

<div class="col-md-6 col-md-offset-3">
<%= render 'form' %>

<% if @test_subcategory.eval_tests.any? %>
    <%= I18n.t(:subcategory_delete_failed) %>
  <% else %>
    <div class="actions">
    <%= link_to link_to t(:button_delete), @test_subcategory, :confirm => t(:button_confirm_delete), method: :delete, class: "btn btn-default" %>
    </div>
  <% end %>
</div>