                <td class="first-content"><%= resultat.athlete_name %></td>
                <td><%= resultat.evaluation_name %></td>
                <td><%= resultat.eval_day %></td>
                <td><%= resultat.test_name %></td>
                <td>
                <% if resultat.left_right? %>
                <%= I18n.t(:resultats_right_abbr) + "- " + resultat.right_side.to_s %>
                <%= I18n.t(:resultats_left_abbr) + "- " + resultat.left_side.to_s %>
                <% else %>
                <%= resultat.right_side %>
                <% end %>
                </td>
              </tr>
          <% end %>
        </tbody>
      </table>
       <%= ADN_Collapsible_Panel_Footer() %>

      <%= ADN_Collapsible_Panel_Header("InjuriesTable", I18n.t(:user_legend_bless_identified)) %>
      <table class="table table-condensed datatable">
