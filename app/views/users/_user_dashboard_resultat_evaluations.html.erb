          <% end %>
        </tbody>
      </table>
      <%= ADN_Collapsible_Panel_Footer() %>

      <%= ADN_Collapsible_Panel_Header("SpecialCaseTable", I18n.t(:user_legend_special_cases) + EvalTest.find(@special_case_eval_test_id).name + I18n.t(:user_legend_special_cases2) + @special_case_value.to_s) %>
      <table class="table table-condensed datatable">
        <thead>
          <tr class="header-table">
            <th><%= t(:general_athlete) %></th>
