<% provide(:title, I18n.t(:user_list)) %>
<h1><%= t :user_list %></h1>

<div class="row">
  <div class="span6 offset3">

    <%= render 'user_search' %>

    <ul class="user">
      <% @users.each do |user| %>
          <li>
            <%= render 'user_index_item', :user => user %>
          </li>
      <% end %>
    </ul>
  </div>
</div>